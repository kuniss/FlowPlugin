package firstflow

import de.grammarcraft.xtend.annotatedflow.Reverse
import de.grammarcraft.xtend.annotatedflow.ToUpper
import de.grammarcraft.xtend.annotatedflow.Collector

flow main
:in -> @ Reverse;
@Reverse -String-> Normalizer;
@Reverse -> @ToUpper;
Normalizer:lower -> @Collector:lower;
Normalizer:upper -> @Collector:upper;
@Collector -> :out;

flow Normalizer
:in -> @de.grammarcraft.xtend.annotatedflow.ToLower;
:in -> @ToUpper;
@de.grammarcraft.xtend.annotatedflow.ToLower -> :lower;
@ToUpper -> :upper;

